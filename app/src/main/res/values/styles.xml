<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <string name="navigation_drawer_open">Open navigation drawer</string>
    <string name="navigation_drawer_close">Close navigation drawer</string>

    <string name="hospital_prompt">Choose a country</string>

    <string-array name="Hospital">
        <item>Colombo</item>
        <item>Peradeniya</item>
        <item>Galle</item>
        <item>Badulla</item>
        <item>Mathara</item>
        <item>Kandy</item>
    </string-array>

    <string name="category_prompt">Choose a country</string>

    <string-array name="Category">
        <item>Anesthesiology</item>
        <item>Cardiology</item>
        <item>Cardiovascular</item>
        <item>Dermatology</item>
        <item>Emergency Medicine</item>
        <item>Gastroenterology</item>
        <item>General Surgery</item>
    </string-array>

</resources>